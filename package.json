  "engines": {
    "node": ">=17.3.1"
  },
  "scripts": {
    "run node": "NODE_OPTIONS=--openssl-legacy-provider hardhat node",
    "compile": "NODE_OPTIONS=--openssl-legacy-provider hardhat compile --show-stack-traces",
    "compile matic": "NODE_OPTIONS=--openssl-legacy-provider hardhat compile --network polygonMumbai",
    "clean types": "NODE_OPTIONS=--openssl-legacy-provider hardhat clean",
    "clean and compile": "NODE_OPTIONS=--openssl-legacy-provider hardhat clean && NODE_OPTIONS=--openssl-legacy-provider hardhat compile",
    "test": "NODE_OPTIONS=--openssl-legacy-provider hardhat test",
    "test matic": "NODE_OPTIONS=--openssl-legacy-provider hardhat test --network polygonMumbai",
    "deploy own script": "NODE_OPTIONS=--openssl-legacy-provider hardhat run ./scripts/deploy.ts --network localhost",
    "deploy script": "NODE_OPTIONS=--openssl-legacy-provider hardhat deploy --export-all ./deployments/deployments.json",
    "deploy own script matic": "NODE_OPTIONS=--openssl-legacy-provider hardhat run ./scripts/deploy.ts --network polygonMumbai",
    "deploy script matic": "NODE_OPTIONS=--openssl-legacy-provider hardhat deploy --network polygonMumbai",
    "accounts": "NODE_OPTIONS=--openssl-legacy-provider hardhat accounts",
    "clientWalletSetup": "NODE_OPTIONS=--openssl-legacy-provider hardhat clientWalletSetup",
    "start deadCoin gatsby client": "npm run develop -w deadcoin",
    "start deadDex client": "npm run dev -w dead-dex",
    "build deadDex client": "npm run build -w dead-dex",
    "start deadDex production": "npm start -w dead-dex"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.13",
